[ng\:cloak]
[ng-cloak]
[data-ng-cloak]
[x-ng-cloak]
.ng-cloak
.x-ng-cloak
  display none !important

.container-non-responsive
  margin-left auto
  margin-right auto
  padding-left 15px
  padding-right 15px
  width 1170px

.nav, .pagination, .carousel, .panel-title a
  cursor pointer

ul
  padding 0

#searchbox
  margin-bottom 0

label
	input[type="checkbox"]
		margin-right 10px

table
	font-size 14px
	text-align inherit
	h4
		font-size 14px

.userMenu
	li
		a
			span
		    i
		    	padding 6px

#accountSidebar
	table
		tr
			&:first-child
				td
					padding-top 20px
					img
						margin-bottom 10px
			&:last-child
				td
					padding-bottom 40px

#accountfFavorites
	.nav-tabs
	  border-bottom 1px solid #BFC9CA
	  li
	  	a
	  		background transparent
	  		font-size 18px
  		&.active,&:hover
  			a
  				color black!important
